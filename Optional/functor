    let Functor =
          https://raw.githubusercontent.com/FormationAI/dhall-bhat/8afbcee6dfd092764df9e9456fe2a2650748dce2/Functor/Type

in    { map =
            λ(a : Type)
          → λ(b : Type)
          → λ(f : a → b)
          → λ(fa : Optional a)
          → Optional/fold
            a
            fa
            (Optional b)
            (λ(x : a) → [ f x ] : Optional b)
            ([] : Optional b)
      }
    : Functor Optional
